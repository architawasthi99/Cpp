#include<iostream>
using namespace std;
int length(char name[]){
    int count=0;
    for(int i=0;name[i]!='\0';i++){
        count++;
    }
    return count;
}
int main(){
    char name[20];
    cout<<"enter the name"<<endl;
    cin>>name;
    cout<<"your name is ";
    cout<<name<<endl;
    cout<<"length is "<<length(name)<<endl;
    return 0;
}
